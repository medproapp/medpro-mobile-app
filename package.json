{
  "name": "app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web-wsl": "EXPO_DEVTOOLS_LISTEN_ADDRESS=0.0.0.0 expo start --web --host lan",
    "start-wsl": "./start-wsl.sh",
    "start-tunnel": "./start-tunnel.sh",
    "dev": "expo start --clear",
    "type-check": "tsc --noEmit",
    "status": "node -e \"const port = process.env.RCT_METRO_PORT || 8081; const { spawn } = require('child_process'); const lsof = spawn('lsof', ['-i', ':' + port]); lsof.stdout.on('data', data => console.log(data.toString())); lsof.on('close', code => { if (code !== 0) console.log('Expo server is not running on port ' + port); });\"",
    "postinstall": ""
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@hookform/resolvers": "^5.1.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "expo": "~54.0.25",
    "expo-asset": "~12.0.10",
    "expo-audio": "~1.0.14",
    "expo-dev-client": "~6.0.18",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.18",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-intent-launcher": "~13.0.7",
    "expo-notifications": "~0.32.13",
    "expo-secure-store": "^15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "react-native": "0.81.5",
    "react-native-blob-util": "^0.23.2",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-pdf": "^7.0.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.21.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@types/react": "~19.1.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-native": "^5.0.0",
    "patch-package": "^8.0.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react-native-safe-area-context": "~5.6.0"
  },
  "private": true
}
